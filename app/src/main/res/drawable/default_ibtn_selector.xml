<?xml version="1.0" encoding="utf-8"?><!-- ImageButton 背景 -->
<inset xmlns:android="http://schemas.android.com/apk/res/android"
    android:insetBottom="0.0px"
    android:insetLeft="0.0px"
    android:insetRight="0.0px"
    android:insetTop="0.0px">
    <selector>
        <item android:state_pressed="true">
            <shape>
                <solid android:color="#FFE85E2D" />
            </shape>
        </item>
        <item>
            <shape>
                <solid android:color="#00000000" />
            </shape>
        </item>
    </selector>
</inset>
